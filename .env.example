# Azure OpenAI Configuration
AZURE_AI_CREDENTIAL=your_azure_api_key
AZURE_AI_ENDPOINT=your_azure_endpoint
AZURE_AI_MODEL_NAME=your_model_name
AZURE_AI_DEPLOYMENT=o4-mini
AZURE_AI_API_VERSION=2024-12-01-preview

# StreamPipes Configuration (optional - for live data)
SP_USERNAME=your_streampipes_username
SP_API_KEY=your_streampipes_api_key

# Langfuse Configuration (optional - for tracking)
# Sign up at https://cloud.langfuse.com to get your keys
LANGFUSE_PUBLIC_KEY=pk-lf-...
LANGFUSE_SECRET_KEY=sk-lf-...
LANGFUSE_HOST=https://cloud.langfuse.com

# Optional Langfuse Settings
# LANGFUSE_RELEASE=v1.0.0                          # Release version for tracking deployments
# LANGFUSE_ENVIRONMENT=production                  # Environment (e.g., development, staging, production)
# LANGFUSE_DEBUG=false                             # Enable debug mode for verbose logging
# LANGFUSE_ENABLED=true                            # Enable/disable tracking globally
# LANGFUSE_SAMPLE_RATE=1.0                         # Sampling rate (0.0-1.0, where 1.0 = 100%)
